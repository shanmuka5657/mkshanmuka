
{
  "name": "CreditWise AI",
  "short_name": "CreditWise",
  "description": "Advanced AI Credit Score Analyzer & ML Model Trainer",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4361ee",
  "icons": [
    {
      "src": "icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "icons/maskable_icon.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "shortcuts": [
    {
      "name": "Analyze Credit Report",
      "url": "/credit",
      "description": "Go to the credit analysis page"
    },
    {
      "name": "Verify Document",
      "url": "/verify",
      "description": "Go to the document verification page"
    }
  ],
  "screenshots": [
    {
      "src": "screenshots/screenshot1.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "screenshots/screenshot2.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "files",
          "accept": [
            "application/pdf"
          ]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "/share-target",
      "accept": {
        "application/pdf": [
          ".pdf"
        ]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+creditwise",
      "url": "/credit?launch_query=%s"
    }
  ],
  "widgets": [
    {
      "name": "Credit Analysis",
      "description": "Quickly access your credit insights.",
      "tag": "credit-widget",
      "template": "credit_widget",
      "ms_ac_template": "credit_widget",
      "data": "{}"
    }
  ],
  "edge_side_panel": {},
  "handle_links": "auto",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "id": "/?utm_source=pwa",
  "categories": [
    "finance",
    "business"
  ],
  "dir": "auto",
  "lang": "en-US",
  "prefer_related_applications": false,
  "iarc_rating_id": "12345",
  "orientation": "portrait",
  "display_override": [
    "window-controls-overlay"
  ]
}
