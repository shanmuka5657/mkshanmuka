{
  "name": "CreditWise AI",
  "short_name": "CreditWiseAI",
  "description": "Advanced AI Credit Score Analyzer & ML Model Trainer",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4361ee",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/maskable_icon_x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icons/maskable_icon_x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "display_override": [
    "standalone",
    "window-controls-overlay"
  ],
  "file_handlers": [
    {
      "action": "/verify",
      "accept": {
        "application/pdf": [
          ".pdf"
        ]
      }
    }
  ],
  "handle_links": "auto",
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "web+creditwise",
      "url": "/credit?type=%s"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "Credit Analysis",
      "short_name": "Credit",
      "description": "Analyze a CIBIL credit report",
      "url": "/credit",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Verify PDF",
      "short_name": "Verify",
      "description": "Run forensic analysis on a PDF",
      "url": "/verify",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "widgets": [
    {
      "name": "CreditWise AI Quick Actions",
      "short_name": "CreditWise",
      "description": "Quick actions for CreditWise AI",
      "tag": "actions",
      "template": "actions",
      "ms_ac_template": "actions",
      "data": {
        "items": [
          {
            "title": "Analyze New Report",
            "action": "analyze",
            "url": "/credit"
          },
          {
            "title": "Verify Document",
            "action": "verify",
            "url": "/verify"
          }
        ]
      },
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  },
  "related_applications": [],
  "scope_extensions": [
    {
      "origin": "*.creditwise.com"
    }
  ],
  "iarc_rating_id": "12345",
  "background_sync": {},
  "periodic_background_sync": {},
  "push_notifications": {}
}
