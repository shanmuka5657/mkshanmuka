{
  "name": "CreditWise AI",
  "short_name": "CreditWiseAI",
  "description": "Advanced AI Credit Score Analyzer & ML Model Trainer",
  "start_url": "/",
  "display": "standalone",
  "background_color": "#ffffff",
  "theme_color": "#4361ee",
  "display_override": [
    "standalone",
    "window-controls-overlay"
  ],
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    },
    {
      "src": "/icons/icon-maskable-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icons/icon-maskable-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "id": "/",
  "launch_handler": {
    "client_mode": [
      "navigate-existing",
      "auto"
    ]
  },
  "orientation": "portrait",
  "screenshots": [
    {
      "src": "/screenshots/screenshot1.png",
      "type": "image/png",
      "sizes": "1080x1920",
      "form_factor": "narrow"
    },
    {
      "src": "/screenshots/screenshot2.png",
      "type": "image/png",
      "sizes": "1080x1920",
      "form_factor": "narrow"
    }
  ],
  "categories": [
    "finance",
    "business",
    "utilities"
  ],
  "dir": "ltr",
  "lang": "en-US",
  "scope": "/",
  "prefer_related_applications": false,
  "related_applications": [],
  "scope_extensions": [
    {
      "origin": "*.creditwiseai.com"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  },
  "file_handlers": [
    {
      "action": "/verify",
      "accept": {
        "application/pdf": [
          ".pdf"
        ]
      }
    }
  ],
  "handle_links": "auto",
  "protocol_handlers": [
    {
      "protocol": "web+creditwiseai",
      "url": "/credit?url=%s"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "Credit Analysis",
      "short_name": "Credit",
      "description": "Analyze a new credit report",
      "url": "/credit",
      "icons": [
        {
          "src": "/icons/shortcut-credit.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Verify PDF",
      "short_name": "Verify",
      "description": "Run forensic analysis on a PDF",
      "url": "/verify",
      "icons": [
        {
          "src": "/icons/shortcut-verify.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "widgets": [
    {
      "name": "CreditWise AI Actions",
      "description": "Quick actions for your credit analysis.",
      "tag": "actions",
      "template": "actions",
      "ms_ac_template": "actions",
      "data": {
        "items": [
          {
            "title": "New Credit Analysis",
            "action": "analyze",
            "icon": "/icons/icon-192x192.png"
          },
          {
            "title": "Verify a Document",
            "action": "verify",
            "icon": "/icons/shortcut-verify.png"
          }
        ]
      },
      "type": "application/json"
    }
  ]
}