{
  "name": "CreditWise AI",
  "short_name": "CreditWise",
  "description": "Advanced AI Credit Score Analyzer & ML Model Trainer",
  "icons": [
    {
      "src": "/icons/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icons/icon-maskable-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "/icons/icon-maskable-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "start_url": "/credit",
  "display": "standalone",
  "background_color": "#f5f7fa",
  "theme_color": "#4361ee",
  "display_override": [
    "standalone",
    "window-controls-overlay"
  ],
  "scope": "/",
  "id": "/?source=pwa",
  "launch_handler": {
    "client_mode": [
      "navigate-existing",
      "auto"
    ]
  },
  "shortcuts": [
    {
      "name": "Credit Analysis",
      "short_name": "Analyze",
      "description": "Analyze a CIBIL credit report",
      "url": "/credit",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Verify PDF",
      "short_name": "Verify",
      "description": "Run forensic analysis on a PDF",
      "url": "/verify",
      "icons": [
        {
          "src": "/icons/icon-192x192.png",
          "sizes": "192x192"
        }
      ]
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "GET",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "screenshots": [
    {
      "src": "/screenshots/screenshot1.png",
      "sizes": "1080x2400",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Credit Analysis Dashboard"
    },
    {
      "src": "/screenshots/screenshot2.png",
      "sizes": "1080x2400",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "AI Chat Agent"
    }
  ],
  "categories": [
    "finance",
    "business"
  ],
  "dir": "auto",
  "lang": "en-US",
  "prefer_related_applications": false,
  "related_applications": [],
  "iarc_rating_id": "12345",
  "file_handlers": [
    {
      "action": "/credit",
      "accept": {
        "application/pdf": [
          ".pdf"
        ]
      }
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+creditwise",
      "url": "/?url=%s"
    }
  ],
  "widgets": [
    {
      "name": "Quick Analysis",
      "description": "Quickly start a new credit analysis.",
      "tag": "analyze",
      "template": "analyze-template",
      "ms_ac_template": "widgets/analyze.json",
      "data": "widgets/analyze-data.json",
      "type": "application/json"
    }
  ],
  "edge_side_panel": {
    "preferred_width": 480
  }
}