{
  "name": "MkCreditWise.com",
  "short_name": "CreditWiseAI",
  "id": "com.mkcreditwise.ai",
  "description": "Advanced AI Credit Score Analyzer & ML Model Trainer by MkCreditWise.com",
  "start_url": "/?source=pwa",
  "scope": "/",
  "display": "standalone",
  "background_color": "#f5f7fa",
  "theme_color": "#4361ee",
  "orientation": "portrait",
  "dir": "ltr",
  "lang": "en-US",
  "categories": [
    "finance",
    "business",
    "productivity"
  ],
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce4e53b7",
  "icons": [
    {
      "src": "/icon-192x192.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "/icon-512x512.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any maskable"
    }
  ],
  "screenshots": [
    {
      "src": "/screenshots/mobile.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow"
    },
    {
      "src": "/screenshots/desktop.png",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide"
    }
  ],
  "related_applications": [
    {
      "platform": "play",
      "url": "https://play.google.com/store/apps/details?id=com.mkcreditwise.ai",
      "id": "com.mkcreditwise.ai"
    }
  ],
  "prefer_related_applications": false,
  "scope_extensions": [
    {
      "origin": "https://*.mkcreditwise.com"
    }
  ],
  "display_override": [
    "window-controls-overlay",
    "minimal-ui"
  ],
  "edge_side_panel": {
    "preferred_width": 400
  },
  "file_handlers": [
    {
      "action": "/process-file",
      "accept": {
        "application/pdf": [
          ".pdf"
        ],
        "application/json": [
          ".json"
        ]
      }
    }
  ],
  "handle_links": "preferred",
  "launch_handler": {
    "client_mode": "focus-existing"
  },
  "protocol_handlers": [
    {
      "protocol": "creditwise",
      "url": "/handle-protocol?url=%s"
    }
  ],
  "share_target": {
    "action": "/share-target",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "New Analysis",
      "short_name": "Analyze",
      "url": "/new-analysis",
      "icons": [
        {
          "src": "/icon-analyze.png",
          "sizes": "96x96"
        }
      ]
    }
  ],
  "widgets": [
    {
      "name": "Credit Score",
      "tag": "credit-score",
      "template": "/widgets/score.html",
      "data": "/widgets/score.json"
    }
  ]
}
